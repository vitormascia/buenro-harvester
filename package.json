{
	"name": "buenro-harvester",
	"version": "1.0.0",
	"description": "Collects data from multiple sources, processes it, and exposes the results through an API.",
	"author": "Vitor Mascia",
	"private": true,
	"type": "module",
	"main": "./build/main.js",
	"license": "UNLICENSED",
	"engines": {
		"node": "24.11.1"
	},
	"scripts": {
		"verify": "npm run ts:nest:compile && npm run lint:fix",
		"build": "npm install && npm run verify",
		"rebuild": "npm clean-install && npm run verify",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node ./build/main.js",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "npm run lint -- --fix",
		"ts:nest:compile": "npx nest build",
		"ts:compile": "rimraf build && tsc --pretty",
		"ts:compile-watch": "rimraf build && tsc --watch --pretty",
		"ts:check": "tsc --noEmit true --pretty",
		"test": "jest --detectOpenHandles --runInBand --logHeapUsage",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"mongo:cli": "docker exec -it mongo_db mongosh",
		"mongo:drop-db": "docker exec mongo_db mongosh --eval 'db.dropDatabase()' buenro_harvester",
		"check:dependencies": "npx depcheck"
	},
	"dependencies": {
		"@fastify/helmet": "^13.0.2",
		"@fastify/static": "^8.3.0",
		"@nestjs/axios": "^4.0.1",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^11.1.9",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.9",
		"@nestjs/mongoose": "^11.0.3",
		"@nestjs/platform-fastify": "^11.1.9",
		"@nestjs/schedule": "^6.0.1",
		"@nestjs/swagger": "^11.2.3",
		"@nestjs/terminus": "^11.0.0",
		"@nestjs/throttler": "^6.4.0",
		"@types/stream-json": "^1.7.8",
		"bignumber.js": "^9.3.1",
		"cache-manager": "^7.2.5",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"got": "^14.6.4",
		"mongoose": "^8.20.0",
		"ms": "^2.1.3",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.1.2",
		"rxjs": "^7.8.2",
		"stream-json": "^1.9.1",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@faker-js/faker": "^10.1.0",
		"@jest/types": "^30.2.0",
		"@nestjs/cli": "^11.0.12",
		"@nestjs/schematics": "^11.0.9",
		"@nestjs/testing": "^11.1.9",
		"@stylistic/eslint-plugin": "^5.6.0",
		"@suites/di.nestjs": "^3.0.1",
		"@suites/doubles.jest": "^3.0.1",
		"@suites/unit": "^3.0.1",
		"@swc/cli": "^0.7.9",
		"@swc/core": "^1.15.2",
		"@types/async": "^3.2.25",
		"@types/jest": "^30.0.0",
		"@types/lodash": "^4.17.20",
		"@types/ms": "^2.1.0",
		"@types/node": "^24.10.1",
		"@types/stream-chain": "^2.1.0",
		"@types/supertest": "^6.0.3",
		"@types/uuid": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"eslint": "^9.39.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-json": "^4.0.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-yml": "^1.19.0",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"jest-mock": "^30.2.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.5",
		"ts-loader": "^9.5.4",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0"
	}
}
